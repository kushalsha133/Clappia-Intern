{{#each products}}
<div class="card card-body text-center border-success bg-light mb-3 container" style="max-width: 45rem;">
    <h4>Product Name: {{productName}}</h4>
    <p>Serial Number: {{productSerialNo}}</p>
    <p>Quantity Available: {{quantity}}</p>
    <p>Price Per Unit: {{pricePerUnit}}</p>

</div> 
{{else}}
<p>No Product Available!</p>
{{/each}}


